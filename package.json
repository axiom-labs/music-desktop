{
  "name": "music-desktop",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.axiom-labs.music",
    "productName": "Axiom Music",
    "mac": {
      "category": "public.app-category.music",
      "darkModeSupport": true
    }
  },
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2"
  },
  "postinstall": "electron-builder install-app-deps"
}
